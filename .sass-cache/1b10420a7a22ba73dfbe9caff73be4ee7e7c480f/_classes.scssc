	}
	&:hover {
		@include opacity(0.8);
	}
}

.button {
	display: inline-block;
	padding: 0 1.11em;
	height: 1.8em;
	line-height: 1.8em;
	font-size: 1.285em;
	background: #e4e4e4;
	border: 1px solid #c2bdbd;
	@include box-shadow(0 1px 0 #333, inset 0 0 5px #fff);
	@extend .dark-grey;
	@extend .gotham-book;
	letter-spacing: -1px;
	margin-bottom: 10px;
	position: relative;

	strong {
		font-weight: normal;
		@extend .gotham-medium;
	}

	&:after {
		@include gt(18);
		@extend .plum;
	}
	
	&.next {
		float: right;
		@extend .dark-grey;
		padding-right: 40px;
		background-image: url(../images/icon_next.png);
		background-repeat: no-repeat;
		background-position: 90% 50%;
		
		&:after {
			content: "";
		}
	}

	&.prev {
		float: left;
		@extend .dark-grey;
		padding-left: 40px;
		background-image: url(../images/icon_prev.png);
		background-repeat: no-repeat;
		background-position: 20px 50%;
		
		&:after {
			content: "";
		}
	}
	&.button:hover {
		background-color: lighten(#e4e4e4, 5%);
		border: 1px solid #c2bdbd;
		@include box-shadow(0 1px 0 #c2bdbd, inset 0 0 5px #fff);
	}

	&.ajax {
		margin-right: 50px;
	}

}

