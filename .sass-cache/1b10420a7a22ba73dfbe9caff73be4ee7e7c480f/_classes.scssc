   ========================================================================== */

.clearfix:before,
.clearfix:after {
    content: " "; /* 1 */
    display: table; /* 2 */
}

.clearfix:after {
    clear: both;
}

.clearfix {
    *zoom: 1;
}

.container:before,
.container:after {
    content: " "; /* 1 */
    display: table; /* 2 */
}

.container:after {
    clear: both;
}

.container {
	width: 68.57142857142857em;
	margin-left: auto;
	margin-right: auto;
	position: relative;
	 *zoom: 1;
}

.column {
	min-height: 1px;
	padding-left: 1.04166666666667%;
	padding-right: 1.04166666666667%;
	float: left;
	display: block;
	position: relative;
	//-webkit-box-sizing: border-box;
	//-moz-box-sizing: border-box;
	//box-sizing: border-box;
	z-index: 100;
}

.alpha.column {
	padding-left: 0px;
}

.omega.column {
	padding-right: 0px;
}
 
$columns: (one, two, three, four, five, six, seven, eight, nine, ten, eleven, twelve, thirteen, fourteen, fifteen, sixteen, seventeen, eighteen, nineteen, twenty);
$total_columns: length($columns);
$column_margin: 1.04166666666667%;
//$pixel_padding = 10;

@mixin columns($width){
	$i: 1;
	@each $column in $columns {
		$total_width: $width;
		@if type-of($width) == number {
			$total_width: ($width * $i) + (($column_margin * 2) * ($i - 1));
		}
		
		.column.#{$column} {
			width: ($total_width);
			padding: $column_margin / ($width);
		}

		.push-#{$column} {
			margin-left: ($total_width + ($column_margin * 2));
		}

		.pull-#{$column} {
			margin-left: -($total_width);
		}
		$i: $i + 1;
	}
}

@include columns(3.0208333333333%);



@include media(1) {
	.container {
		width: 90%;
		padding: 0 5%;
	}
	@include columns(60% / $total_columns);
}


.left, .alignleft {
	float: left;
}

.right, .alignright {
	float: right;
}

/* =============================================================================
   Table
   ========================================================================== */

table {
	
}
	table td{
		padding: 5px 0;
	}

/* =============================================================================
   Forms
   ========================================================================== */

input[type=text],
input[type=email],
input[type=password], 
textarea,
select{
	@include box-sizing(border-box);
	color: #666;
	border: none;
}

input[type=text]:focus,
input[type=email]:focus {
	outline: none;
}

label {
	display: block;
	color: #666;
}

label.error {
	color: #F00;
}

input[type=text],
input[type=email],
input[type=password],
input[type=tel],
textarea {
	background: #FFF;
	padding: 8px 6px;
	width: 100%;
	max-width: 100%;
}

select {
	padding: 4px;
	width: 166px;
}

textarea {
	height: 200px;
}
select,
input[type=text],
input[type=email],
input[type=password],
input[type=submit],
input[type=reset],
textarea {
	margin: 5px 0;
}

input.numbers-only {
	width: 70px;
}

input[type=submit],
input[type=reset] {
	border: none;
	cursor: pointer;	
	color: #FFFFFF;
}

button::-moz-focus-inner,
input[type="reset"]::-moz-focus-inner,
input[type="button"]::-moz-focus-inner,
input[type="submit"]::-moz-focus-inner,
input[type="radio"]::-moz-focus-inner,
option::-moz-focus-inner,
select::-moz-focus-inner,
input[type="file"] > input[type="button"]::-moz-focus-inner {
	border: none !important;
	outline: none !important;
}

