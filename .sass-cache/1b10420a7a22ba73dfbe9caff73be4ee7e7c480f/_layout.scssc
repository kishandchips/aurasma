	}

	ol {
		padding-left: 20px;
		line-height: 1.5em;
	}
	ul {
		padding-left: 0;
	}
	ul li {
		display: inline-block;
		width: 50%;
		float: left;

		a:before {
			@include backslash(1);
		}

	}
}

#news {

	#main {
		background: url(../images/bg_stripes.png) repeat 0 0;
		padding: 0 0 65px;
	}

	#top-content {

		div.box {

			h3 {
				@extend .violet;

				&:before {
					@extend .red;
				}
			}
		}
	}


	#category {

		padding-top: 60px;

		h2 {
			@extend .gotham-book;
			@extend .violet;
			font-size: 2.5em;
		}

		h3 {

			font-size: 1.571em;
			line-height: 110%;
			padding-bottom: 5px;
			max-width: 90%;
			@extend .dark-grey;

			&:before {
				content: "";
			}
		}

		.post {

			margin-bottom: 40px;
			background-color: #fff;
			@include box-shadow(0 1px 5px #666);
			@extend .dark-grey;

			.post-title {

				background-color: rgba(#000, 0.6);
				z-index: 1000;
				width: 100%;


				h2 {

					padding: 15px 29px;
					line-height: 100%;
					font-size: 3.214em;
					margin: 0;
					max-width: 91%;
					@extend .white;

					a {
						@extend .white;

						&:hover {
							@extend .light-grey;
						}
					}
				}

			}

			.featured-image {

				position: relative;
				height: 368px;
				overflow: hidden;
				width: 100%;

				img {
					width: 666px;
					height: auto;
				}

				.post-title {

					position: absolute;
					bottom: 0;

				}

			}

			.post-meta {

				margin-bottom: 30px;
				font-size: 0.928em;

				span {

					&.date:after {

				 		content: "\0000a0\0000a0\\";
				 		@extend .light-grey;

					}

					&.the-category {
						padding-left: 10px;
					}

				}

				a {
					@extend .violet;

					&:hover {
						@extend .purple;
					}
				}

			}

			.post-text {

				padding: 19px 60px 50px 29px;
				line-height: 1.5em;

				p {
					margin: 10px 0;
				}

				a {
					color: #bb006b;

					&:hover {
						color: #a0005b;
					}
				}

			}

		}

	}

	div#sidebar {

		padding-top: 60px;

		h3 {

			@extend .violet;

			&:before {
				@include backslash(1, 20px);
				@extend .red;
			}	
		}

		ul {

			padding: 0;

			li {

				@extend .gotham-medium;
				list-style: none;
				font-size: 1.142em;
				line-height: 140%;

				a {
					@extend .dark-grey-purple-hover;
				}
			}

			li.cat-item{

				&:before {
					@include backslash(1);
					@extend .dark-grey;
				}	

			}
		}

		.categories {

			li {

				&:before{
					@include backslash(1, 12px);
					@extend .dark-grey-purple-hover;
				}
			}
		}

		.follow-us {

			li {

				padding-left: 30px;
				background-repeat: no-repeat;
				background-position: 0 50%;
				line-height: 170%;

				&.facebook {
					background-image: url(../images/icon_facebook_grey.png);
				}

				&.twitter {
					background-image: url(../images/icon_twitter_grey.png);
				}

				&.youtube {
					background-image: url(../images/icon_youtube_grey.png);
				}

				&.pinterest {
					background-image: url(../images/icon_pinterest_grey.png);
				}

			}

		}

	}

	.newsletter input[type="submit"] {
		left: 20em;
}

}

#campaigns, .single-campaign {

	.load-more {
		@extend .push-six;
		margin-top: 20px;
	}

	#main {
		background: url(../images/bg_stripes.png) repeat 0 0;
		padding: 0 0 65px;
	}

	#slider {
		background: url(../images/bg_slider.png) repeat-x top left;
		@extend .mid-purple-border;

		h2 {
			@extend .gotham-book;
			@extend .white;
			@include text-shadow(0 2px 0 #281a27);
		}
		h3 {
			@extend .white;
			font-size: 1.428em;
			@include text-shadow(0 2px 0 #281a27);
			
			&:before {
				content: "";
			}
		}
	}

	#category {

		width: 68.57142857142857em;
		margin: 0 auto;
		padding-top: 60px;

		h2 {
			@extend .gotham-book;
			@extend .violet;
			font-size: 2.5em;
		}
		h3 {

			font-size: 1.428em;
			@extend .dark-grey;

			&:before {
				@include backslash(1);
				@extend .plum;
			}
		}
		div.thumbnails {
			margin-top: 50px;

			.post {
				img {
					@include media(7){
						max-width: 100%;
						height: auto;
					}
					
				}

				a:before {
					content: " ";
					background: url(../images/single_campaign_hover_176x176.png) no-repeat center center;
					width: 100%;
					height: 100%;
					position: absolute;
				}

				a:hover {
					&:before {
						@include opacity(1);
					}
				}
			}
		}
	}

	.thumbnails {
		
	}
}


.campaign {

	.close {
		margin-bottom: 10px;
	}

	.post-text {

		* {
			color: #fff;
		}

		hgroup {
			margin-bottom: 10px;
		}

		h2.post-title {
			margin-bottom: 10px;
		}

		h5.post-item {
			font-weight: bold;
			font-size: 1em;
		}

		h3.post-item {
			font-size: 1.428em;

			&:before {
				@include backslash(0);
			}
		}

		p {
			margin: 8px 0;
		}

		.foot-meta {

			margin-top: 20px;

			.foot-links {
				padding: 0;

				li {
					list-style: none;
					line-height: 160%;

					a {
						color: #d4bedb;
						font-weight: bold;

						&:hover {
					
							@extend .white;
						}

					}

					&:before {
						@include backslash(1, 14px);
					}

					em {
						font-size: 0.857em;
						font-style: normal;

						&:before {
							@include backslash(1, 12px);
						}

					}

				}
			}


			.channel {
				display: inline-block;
				background: url(../images/icon_search_white.png) no-repeat;
				padding: 0 0 0 25px;
				line-height: 20px;
				height: 20px;
				font-weight: bold;
			}

		}

		.share-icons {

			padding: 0;

			li {

				display: inline-block;
				width: 32px;
				height: 32px;
				text-indent: -999em;
				@include border-radius();
				border: 1px solid #999;

			}

		}
		.app {

			img {
				max-width: 9em;
				height: auto;
			}

		}
	}

	.wp-post-image {
		width: 100%;
		height: auto;
	}



}

#about-us {

	#main {

		background: url(../images/bg_stripes.png) repeat 0 0;
		padding-top: 0;

	}

 	.contacts {
		p {
			font-size: 1.142em;
		}

		a {
			@extend .violet;
			@extend .gotham-medium;
		}
	}


	.where-to-find-us {
		h3 {
			font-size: 1.785em;
			@extend .violet;
			margin: 15px 0 5px 0;

			&:before {
				@include backslash(0);
			}
		}

		p {
			font-size: 1.071em;
			margin: 0;
		}

		a.google-maps {
			font-size: 1.142em;
			display: block;
			@extend .violet;
			@extend .gotham-medium;
			margin-top: 20px;
			padding-left: 25px;
			background: url(../images/icon_maps.png) no-repeat 0 50%;
		}

		a img {
			width: 100%;
			height: auto;
		}
	}

	.history-and-technology, .our-history {
		h4 {
			font-size: 1.571em;
		}

		img {
			max-width: 100%;
			height: auto;
		}

	}

	h2 {
		font-size: 2.5em;
		@extend .violet;

		&:before {
			@include backslash(1, 26px);
			@extend .dark-purple;
		}

	}

	#category {

		margin: 0 auto;
		width: 68.57142857142857em;

		article {

			border-bottom: 1px solid #b5b5b5;
			padding: 50px 0;
			margin-bottom: 20px;

		}

		form {

			div {

				margin-bottom: 10px;

				label {
					@extend .gotham-medium;
					cursor: pointer;
				}

				input[type="text"], input[type="email"], textarea {
					@extend .gotham-book;
					border: 1px solid #818181;
					@include border-radius(3px);
				}

				input[type="text"], input[type="email"] {
					@extend .nine.column;
				}

				input[type="submit"] {
					@extend .button;
					float: right;
				}

			}

		}

	}

	#top-content {

		h3 {
			@extend .violet;

			&:before {
				@extend .dark-purple;
			}
		}
	}
}

div.thumbnails.team {

	.post {

		height: auto;
		margin-bottom: 40px;

		a {
			position: relative;
			display: inline;
			height: auto;
			//width: 176px;
			background: none;
			padding-right: 5px;
			@include box-shadow(none);	
			@include transition(none);

			&:hover, &.selected {
				@include scale(0);
			}
		}

		h4 {
			@extend .violet;
			@extend .gotham-medium;
			font-size: 1.428em;
			margin: 10px 0 0 0;
		}
		h5 {
			@extend .dark-grey;
			@extend .gotham-medium;
			font-size: 1.071em;
			margin: 0 0 10px 0;
		}

	}
}

body#partners-faqs {
	.close {
		display: none;
	}

}

#become-a-partner {
	#main {
		padding-bottom: 0;

		article {

			margin: 0 0 20px 0;

			img {
				max-width: 100%;
				height: auto;
				margin-top: 1em;
			}
		}
	}
}

body#news.single-partner, body#news.single-campaign {
	
	#main {
		padding-top: 45px;

		.close, .prev.button, .next.button {
			display: none;
		}

	}
}
body#news.single-campaign {
	
	.post-text * {
		color: #333;

		h2 {
			@extend .violet;
			margin-bottom: 20px;
		}
	}

	ul.foot-links a {
		@extend .grey-purple-hover;
	}
}

#news.search {
	#category {
		.post {

			.post-title {
				background: #666;
				h2 {
					font-size: 2.5em;
				}
			}
			.post-text {
				padding-bottom: 30px;
			}
		}

	}

}

#top-content .search-form, #main .search-form {

	@extend .fourteen.column;

	input[type="text"]{
		width: 83%;
		padding: 7px 6px;
		@extend .gotham-book;
		border: 1px solid #818181;
		@include border-radius(3px);
	}

	input[type="submit"] {
		@extend .button;
		float: right;
	}
}


.meet-the-team {
	.post {
		img {
			max-width: 93%;
			height: auto;
		}
	}
}


#partners #menu-item-129 a,
#campaigns #menu-item-33 a,
.single-partner #menu-item-129 a,
.single-campaign #menu-item-33 a,
#become-a-partner #menu-item-129 a,
#partners-faqs #menu-item-129 a {
	@extend .purple;
}







