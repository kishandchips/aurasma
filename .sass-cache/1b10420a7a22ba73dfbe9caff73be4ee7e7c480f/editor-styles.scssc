/* =============================================================================
   Buttons
   ========================================================================== */

.small-button {
	@extend .hide-text;
	display: block;
	float: left;
	margin-right: 5px;
	width: 2.68em;
	height: 2.68em;
	background-position: 50% 50%;
	background-repeat: no-repeat;
	background-size: cover;

	&.facebook {
		background-image: url(../images/icon_facebook.png);
	}
	&.twitter {
		background-image: url(../images/icon_twitter.png);
	}
	&.youtube {
		background-image: url(../images/icon_youtube.png);
	}
	&.pinterest {
		background-image: url(../images/icon_pinterest.png);
	}
}

.button {
	display: inline-block;
	padding: 0 1.11em;
	height: 1.8em;
	line-height: 1.8em;
	font-size: 1.285em;
	background: #e4e4e4;
	border: 1px solid #c2bdbd;
	@extend .dark-grey;
	@extend .gotham-book;
	letter-spacing: -1px;
	margin-bottom: 10px;
	position: relative;

	strong {
		font-weight: normal;
		@extend .gotham-medium;
	}

	&:after {
		@extend .plum;
	}
	
	&.next {
		float: right;
		@extend .dark-grey;
		padding-right: 40px;
		background-image: url(../images/icon_next.png);
		background-repeat: no-repeat;
		background-position: 90% 50%;
		
		&:after {
			content: "";
		}
	}

	&.prev {
		float: left;
		@extend .dark-grey;
		padding-left: 40px;
		background-image: url(../images/icon_prev.png);
		background-repeat: no-repeat;
		background-position: 20px 50%;
		
		&:after {
			content: "";
		}
	}
	&.button:hover {
		background-color: lighten(#e4e4e4, 5%);
		border: 1px solid #c2bdbd;
	}

	&.ajax {
		margin-right: 50px;
	}

}

.close-button {
	display: inline-block;
	float: right;
	height: 20px;
	font-size: 1.285em;
	@extend .gotham-book;
	@extend .medium-grey;
	padding-left: 30px;
	background: url(../images/icon_close.png) no-repeat 0 50%;
}

.up-button {
	display: inline-block;
	float: right;
	font-size: 1.285em;
	@extend .gotham-book;
	@extend .medium-grey;
	padding-left: 30px;
	background: url(../images/icon_arrow_up.png) no-repeat 0 50%;
	position: relative;
	z-index: 2000;
}

.external_url {
	display: block;
	padding-left: 20px;
	background: url(../images/icon_external.gif) no-repeat 0 50%;
	font-weight: bold;
	@extend .dark-grey;
	margin-top: 30px;
	margin-bottom: -25px;

	em {
		font-weight: normal;
		font-style: normal;
		@extend .light-grey;
	}
}
.download {
	display: block;
	padding-left: 20px;
	background: url(../images/icon_download.png) no-repeat 0 50%;
	font-weight: bold;
	@extend .dark-grey;
	margin-top: 30px;

	em {
		font-weight: normal;
		font-style: normal;
		@extend .light-grey;
	}
}

.no-click {
	cursor: default;
}

.purple-btn {
	padding: 5px 10px;
	display: inline-block;
	background: #9C499C;
	background: rgba(156, 73, 156, 0.7);
	color: #FFF !important;
	border-radius: 20px;
	font-family: 'gothamMedium';

	&:hover {
		background: #9C499C;
	}
}

/* =============================================================================
   Grid
   ========================================================================== */

.clearfix:before,
.clearfix:after {
    content: " "; /* 1 */
    display: table; /* 2 */
}

.clearfix:after {
    clear: both;
}

.clearfix {
    *zoom: 1;
}

.container:before,
.container:after {
    content: " "; /* 1 */
    display: table; /* 2 */
}

.container:after {
    clear: both;
}

.container {
	width: 68.57142857142857em;
	margin-left: auto;
	margin-right: auto;
	position: relative;
	 *zoom: 1;
}

.column {
	min-height: 10px;
	padding-left: 1.04166666666667%;
	padding-right: 1.04166666666667%;
	float: left;
	display: block;
	position: relative;
	//-webkit-box-sizing: border-box;
	//-moz-box-sizing: border-box;
	//box-sizing: border-box;
	z-index: 100;
}

.alpha.column {
	padding-left: 0px;
}

.omega.column {
	padding-right: 0px;
}
 
$columns: (one, two, three, four, five, six, seven, eight, nine, ten, eleven, twelve, thirteen, fourteen, fifteen, sixteen, seventeen, eighteen, nineteen, twenty);
$total_columns: length($columns);
$column_margin: 1.04166666666667%;
//$pixel_padding = 10;

@mixin columns($width){
	$i: 1;
	@each $column in $columns {
		$total_width: $width;
		@if type-of($width) == number {
			$total_width: ($width * $i) + (($column_margin * 2) * ($i - 1));
		}
		
		.column.#{$column} {
			width: ($total_width);
			padding: $column_margin / ($width);
		}

		.push-#{$column} {
			margin-left: ($total_width + ($column_margin * 2));
		}

		.pull-#{$column} {
			margin-left: -($total_width);
		}
		$i: $i + 1;
	}
}

@include columns(3.0208333333333%);




